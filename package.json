{
  "name": "openapi-schema-generator",
  "version": "1.0.0",
  "description": "Generates an OpenAPIv3 json schema from typescript interfaces with JSDOC annotations representing API endpoint parameters",
  "type": "module",
  "main": "./dist/index.cjs",
  "bin": "./dist/index.cjs",
  "scripts": {
    "test": "ts-node --esm ./src/index.ts",
    "build": "rimraf ./dist && rollup -c"
  },
  "author": "William M.",
  "license": "ISC",
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/lodash": "^4.14.196",
    "commander": "^11.0.0",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
